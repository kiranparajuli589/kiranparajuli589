#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/htmlmark@0.1.3/node_modules/htmlmark/lib/cli/node_modules:/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/htmlmark@0.1.3/node_modules/htmlmark/lib/node_modules:/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/htmlmark@0.1.3/node_modules/htmlmark/node_modules:/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/htmlmark@0.1.3/node_modules:/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/htmlmark@0.1.3/node_modules/htmlmark/lib/cli/node_modules:/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/htmlmark@0.1.3/node_modules/htmlmark/lib/node_modules:/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/htmlmark@0.1.3/node_modules/htmlmark/node_modules:/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/htmlmark@0.1.3/node_modules:/Users/kiran/IdeaProjects/kiranparajuli589/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli/index.js" "$@"
else
  exec node  "$basedir/../../lib/cli/index.js" "$@"
fi
